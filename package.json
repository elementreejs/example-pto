  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjstahl/elementree-pto/issues"
  },
  "homepage": "https://github.com/mjstahl/elementree-pto#readme",
  "dependencies": {
    "elementree": "^0.23.5",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "steal": "^2.2.1",
    "steal-less": "^1.3.4",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@mjstahl/other-index": "^0.2.2",
    "standard": "^12.0.1"
